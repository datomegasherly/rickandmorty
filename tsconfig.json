{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@css/*": ["src/css/*"],
      "@actions": ["src/redux/actions.ts"],
      "@queries": ["src/queries/index.ts"],
      "@config": ["config.ts"]
    }
  }
}
